алг тест_функций;
нач
    а : цел;
    б : цел;
кон
функции
    функция сумма(х : цел, у : цел) : цел;
    нач
        результат : цел;
    кон
        результат := х + у;
        знач результат;
    кон
    
    функция вывести_число(число : цел);
    кон
        вывод("Число:");
        вывод(число);
    кон
кон
    а := 10;
    б := 20;
    а := сумма(а, б);
    вывести_число(а);
кон 