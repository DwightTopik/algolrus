алг тест_функций_исправленный;
нач
    а : цел;
    б : цел;
    результат : цел;
кон
функции
    функция сумма(х : цел, у : цел) : цел;
    нач
        temp : цел;
    кон
        temp := х + у;
        знач temp;
    кон
    
    функция вывести_число(число : цел);
    нач
        вывод("Число: ");
        вывод(число);
    кон
кон
    а := 10;
    б := 20;
    результат := сумма(а, б);
    вывести_число(результат);
    вывод("Тест функций завершён!");
кон 